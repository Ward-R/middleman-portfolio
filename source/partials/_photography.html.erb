<section id="photography-page">
  <h1>Photography</h1>
  <div class="photo-display-area">
    <div class="image-aspect-ratio-wrapper">
      <img id="main-photo" src="/images/photography/1-DSCF3844_u4cobo.jpg" alt="Current Photo">
    </div>
    <div class="photo-navigation">
      <button id="prevBtn">Prev</button> <%# Make sure ID is prevBtn %>
      <button id="nextBtn">Next</button> <%# Make sure ID is nextBtn %>
    </div>
  </div>
</section>
<% content_for :scripts_for_photography do %>
  <%= javascript_include_tag "photography_viewer" %>
<% end %>
